{
  "name": "google-cast-launcher",
  "version": "0.0.1",
  "description": "google-cast-launcher",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:watch": "npm run build -- --watch",
    "clean": "npm run clean:build",
    "clean:build": "rimraf build/",
    "dev": "concurrently \"npm run build:watch\" \"npm run start:watch\"",
    "lint": "eslint '{src,tests}/**/*.ts'",
    "start": "node build/index.js",
    "start:watch": "nodemon build/index.js --log"
  },
  "author": "Timo Lyytinen",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "12.0.1",
    "@types/puppeteer-core": "1.9.0",
    "@types/yargs-parser": "13.0.0",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "concurrently": "4.1.0",
    "eslint": "5.16.0",
    "eslint-plugin-prettierx": "0.3.1",
    "nodemon": "1.19.0",
    "rimraf": "2.6.3",
    "typescript": "3.4.5",
    "yargs-parser": "13.1.0"
  },
  "dependencies": {
    "chrome-remote-interface": "0.27.1",
    "puppeteer-core": "1.16.0",
    "fastify": "2.3.0"
  }
}
