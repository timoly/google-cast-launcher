    "@types/yargs-parser": "13.0.0",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "concurrently": "4.1.0",
    "eslint": "5.16.0",
    "eslint-plugin-prettierx": "0.3.1",
    "nodemon": "1.19.0",
    "rimraf": "2.6.3",
    "typescript": "3.4.5",
    "yargs-parser": "13.1.0"
  },
